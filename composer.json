{
    "name": "laravel-notification-channels/facebook-poster",
    "description": "Use notification to create posts on Facebook",
    "license": "MIT",
    "authors": [{
        "name": "Ahmed Ashraf",
        "email": "ahmed29329@gmail.com"
    }],
    "require": {
        "facebook/graph-sdk": "^5.4",
        "illuminate/notifications": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.3|~3.4"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\FacebookPoster\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\FacebookPoster\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\FacebookPoster\\FacebookPosterServiceProvider"
            ]
        }
    }
}
